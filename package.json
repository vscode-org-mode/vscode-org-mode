{
    "name": "org-mode",
    "displayName": "Org Mode",
    "description": "Port of Emacs Org Mode",
    "version": "0.0.1",
    "publisher": "tootone",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:org"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.insertSibling",
                "title": "Org-mode: org-insert-heading-respect-content"
            },
            {
                "command": "extension.insertChild",
                "title": "Org-mode: Insert Child"
            },
            {
                "command": "extension.insertTimestamp",
                "title": "Org-mode: Timestamp"
            },
            {
                "command": "extension.bold",
                "title": "Org: Bold"
            },
            {
                "command": "extension.italic",
                "title": "Org: Italic"
            },
            {
                "command": "extension.underline",
                "title": "Org: Underline"
            },
            {
                "command": "extension.code",
                "title": "Org: Code"
            },
            {
                "command": "extension.verbose",
                "title": "Org: Verbose"
            },
            {
                "command": "extension.literal",
                "title": "Org: Literal"
            }
        ],
        "languages": [
            {
                "id": "org",
                "aliases": [
                    "Org",
                    "Org Markup",
                    "org",
                    "org-mode"
                ],
                "extensions": [
                    ".org"
                ],
                "configuration": "./org.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "org",
                "scopeName": "source.org",
                "path": "./syntaxes/org.tmLanguage.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}